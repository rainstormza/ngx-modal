{
  "name": "ngx-modal2",
  "version": "4.3.0",
  "description": "Open modal window for your Angular X application",
  "scripts": {
    "start": "webpack-dev-server --hot --progress --colors",
    "start:aot": "npm run start -- --env.aot",
    "gulp": "gulp",
    "karma": "karma start",
    "t": "npm run karma -- --single-run",
    "test": "npm run t",
    "test:w": "npm run karma",
    "build": "ng-packagr -p ng-package.json",
    "postbuild": "npm run gulp -- postbuild",
    "tsc": "tsc",
    "lint": "npm run eslint -- src --ext .ts && npm run tsc -- --noEmit --noUnusedLocals",
    "eslint": "eslint",
    "np": "cd package && npm publish",
    "release": "npm run test && npm run lint && npm run build && npm run np"
  },
  "keywords": [
    "modal",
    "ngx",
    "angular2"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "typings": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/ngx-modal.git"
  },
  "homepage": "https://github.com/unlight/ngx-modal",
  "devDependencies": {
    "@angular/common": "4.2.5",
    "@angular/compiler": "4.2.5",
    "@angular/compiler-cli": "4.2.5",
    "@angular/core": "4.2.5",
    "@angular/platform-browser": "4.2.5",
    "@angular/platform-browser-dynamic": "4.2.5",
    "@angular/router": "4.2.5",
    "@ngtools/webpack": "1.5.0",
    "@types/del": "^2.2.32",
    "@types/jasmine": "2.5.53",
    "@types/node": "8.0.7",
    "angular-router-loader": "^0.6.0",
    "awesome-typescript-loader": "3.2.1",
    "core-js": "2.4.1",
    "css-loader": "0.28.4",
    "css-to-string-loader": "^0.1.3",
    "del": "^3.0.0",
    "ejs-loader": "0.3.0",
    "eslint": "4.1.1",
    "eslint-plugin-import": "2.6.1",
    "eslint-plugin-jasmine": "2.6.2",
    "eslint-plugin-perf-standard": "^1.0.3",
    "eslint-plugin-prefer-spread": "^1.0.3",
    "eslint-plugin-typescript": "^0.1.0",
    "eslint-plugin-unicorn": "2.1.2",
    "eslint-plugin-wix-editor": "^1.1.1",
    "file-loader": "^0.11.2",
    "gulp": "3.9.1",
    "gulp-ignore": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "html-webpack-plugin": "2.29.0",
    "jasmine-core": "2.6.4",
    "karma": "1.7.0",
    "karma-jasmine": "1.1.0",
    "karma-nightmare": "0.4.9",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "ng-packagr": "1.0.0-alpha.3",
    "raw-loader": "0.5.1",
    "rxjs": "5.4.1",
    "style-loader": "^0.18.2",
    "ts-node": "3.1.0",
    "tslib": "^1.7.1",
    "typescript": "2.4.1",
    "typescript-eslint-parser": "^3.0.0",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.5.0",
    "zone.js": "^0.8.12"
  }
}
